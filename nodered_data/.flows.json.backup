    {
        "id": "2",
        "type": "function",
        "z": "6a9b87f63a2da681",
        "name": "Generar datos",
        "func": "const temp = (Math.random() * (30 - 15) + 15).toFixed(1);  // Temperatura entre 15 y 30 °C\nconst battery = Math.max(0, (msg.batteryLevel || 0.75) - 0.01).toFixed(2); // Descarga progresiva\n\nmsg.method = \"PATCH\"\n\nmsg.payload = {\n    value: {\n        type: \"Number\",\n        value: parseFloat(temp)  // Solo la temperatura\n    },\n    batteryLevel: {\n        type: \"Number\",\n        value: parseFloat(battery)\n    }\n};\n\nmsg.headers = {\n    \"Content-Type\": \"application/json\"\n};\n\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
